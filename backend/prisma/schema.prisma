generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Lead {
  id              Int      @id @default(autoincrement())
  nome            String
  email           String   @unique
  telefone        String
  cargo           String
  data_nascimento DateTime
  mensagem        String   @db.Text
  utm_source      String?
  utm_medium      String?
  utm_campaign    String?
  utm_term        String?
  utm_content     String?
  gclid           String?
  fbclid          String?
  createdAt       DateTime @default(now())
}
